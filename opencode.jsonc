{
  "mcpServers": {
    "grep-app": {
      "command": "npx",
      "args": [
        "-y",
        "@grepapp/mcp-server-grep-app"
      ],
      "env": {
        "GREP_APP_API_KEY": "YOUR_GREP_APP_API_KEY_HERE"
      }
    },
    "context7": {
      "command": "npx",
      "args": [
        "-y",
        "@context7/mcp-server"
      ],
      "env": {
        "CONTEXT7_API_KEY": "YOUR_CONTEXT7_API_KEY_HERE"
      }
    }
  }
}
